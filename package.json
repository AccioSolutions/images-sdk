{
  "name": "@acciosolutions/images",
  "description": "Package to upload images. Powered by AccioSolutions",
  "version": "0.0.3",
  "license": "MIT",
  "author": "ChristianCuri",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AccioSolutions/images-sdk.git"
  },
  "keywords": [
    "images",
    "acciosolutions",
    "accio",
    "storage",
    "upload-image",
    "image-upload",
    "upload"
  ],
  "bugs": {
    "url": "https://github.com/AccioSolutions/images-sdk/issues"
  },
  "homepage": "https://github.com/AccioSolutions/images-sdk#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "build": "yarn build:clean && tsc",
    "build:clean": "rimraf dist",
    "lint:fix": "npm run lint -- --fix",
    "lint": "eslint ./src --ext .ts",
    "prerelease": "npm run build",
    "release": "git add --force dist/ && standard-version -a"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "form-data": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@sentry/types": "^5.27.3",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/express-fileupload": "^1.1.3",
    "@types/helmet": "^0.0.47",
    "@types/mongoose": "^5.7.37",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.27",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "esbuild-node-tsc": "^1.0.4",
    "eslint": "^7.13.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "standard-version": "^7.0.0",
    "ts-node": "^9.0.0",
    "tsd": "^0.13.1",
    "typescript": "^4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn lint"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "tsd": {
    "directory": "./src",
    "compilerOptions": {
      "rootDir": "./src"
    }
  }
}
